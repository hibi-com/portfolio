{
    "[dockerfile]": {
        "editor.formatOnSave": false
    },
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[json]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.detectIndentation": false,
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[jsonc]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.detectIndentation": false,
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[markdown]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.quickSuggestions": {
            "comments": "on",
            "other": "on",
            "strings": "on"
        },
        "editor.snippetSuggestions": "top",
        "editor.unicodeHighlight.ambiguousCharacters": true,
        "editor.unicodeHighlight.invisibleCharacters": true,
        "editor.wordWrap": "on",
        "files.insertFinalNewline": false,
        "files.trimTrailingWhitespace": false
    },
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[yaml]": {
        "editor.formatOnSave": false
    },
    "biome.enabled": true,
    "biome.lsp.bin": {
        "darwin-arm64": "./node_modules/@biomejs/cli-darwin-arm64/biome",
        "darwin-x64": "./node_modules/@biomejs/cli-darwin-x64/biome",
        "linux-x64": "./node_modules/@biomejs/cli-linux-x64/biome",
        "win32-x64": "./node_modules/@biomejs/cli-win32-x64/biome.exe"
    },
    "biome.requireConfiguration": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.biome": "explicit",
        "source.organizeImports.biome": "explicit"
    },
    "editor.defaultFormatter": "biomejs.biome",
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.associations": {
        "*.dbclient-js": "javascript",
        "plyconfig.json": "jsonc"
    },
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.cache": true,
        "**/.git": true,
        "**/.tanstack": true,
        "**/.turbo": true,
        "**/.wrangler": true,
        "**/Thumbs.db": true,
        "**/node_modules": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.cache/**": true,
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.tanstack/**": true,
        "**/.turbo/**": true,
        "**/.wrangler/**": true,
        "**/build/**": true,
        "**/dist/**": true,
        "**/node_modules/**": true,
        "**/target/**": true
    },
    "git.autoRepositoryDetection": "openEditors",
    "git.detectSubmodules": false,
    "git.ignoreLimitWarning": true,
    "git.ignoreSubmodules": true,
    "npm.packageManager": "bun",
    "search.exclude": {
        "**/.cache": true,
        "**/.git": true,
        "**/.tanstack": true,
        "**/.turbo": true,
        "**/.wrangler": true,
        "**/build": true,
        "**/dist": true,
        "**/node_modules": true
    },
    "search.followSymlinks": false,
    "search.quickOpen.includeSymbols": false,
    "search.seedWithNearestWord": true,
    "search.smartCase": true,
    "search.useIgnoreFiles": true,
    "sonarlint.connectedMode.project": {
        "connectionId": "ageha734",
        "projectKey": "ageha734_portfolio"
    },
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.preferences.importModuleSpecifier": "relative",
    "typescript.suggest.autoImports": true,
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.workspaceSymbols.scope": "allOpenProjects",
    "vitest.maximumConfigs": 15
}
