{
  "private": true,
  "version": "2.0.0",
  "name": "@portfolio/admin",
  "type": "module",
  "scripts": {
    "build": "bun run with-env vite build",
    "clean:build": "git clean -xdf .tanstack .wrangler build",
    "clean:cache": "git clean -xdf .cache .turbo",
    "clean:pkg": "git clean -xdf node_modules build",
    "coverage": "vitest run --coverage",
    "dev": "bun run with-env vite",
    "e2e": "docker run --rm -e CI=true -e APP_DIR=apps/admin -v $(git rev-parse --show-toplevel):/work -w /work -v $(git rev-parse --show-toplevel)/node_modules:/work/node_modules -v $(git rev-parse --show-toplevel)/docs/playwright/report:/work/docs/playwright/report -v $(git rev-parse --show-toplevel)/docs/security/e2e:/work/docs/security/e2e e2e",
    "fmt": "biome format --write .",
    "fmt:check": "biome format .",
    "generate": "wrangler types",
    "integration": "vitest run -c vitest.integration.config.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "with-env": "env"
  },
  "dependencies": {
    "@portfolio/api": "workspace:*",
    "@portfolio/cms": "workspace:*",
    "@portfolio/log": "workspace:*",
    "@portfolio/ui": "workspace:*",
    "@tanstack/react-router": "1.147.3",
    "axios": "1.7.9",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260101.0",
    "@portfolio/biome-config": "workspace:*",
    "@portfolio/playwright-config": "workspace:*",
    "@portfolio/tsconfig": "workspace:*",
    "@portfolio/vite-config": "workspace:*",
    "@portfolio/vitest-config": "workspace:*",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/router-plugin": "1.147.3",
    "@tanstack/router-vite-plugin": "1.147.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "5.1.2",
    "dotenv": "17.2.3",
    "react-router": "7.11.0",
    "typescript": "5.9.3",
    "vite": "6.1.0",
    "vitest": "3.2.4",
    "wrangler": "4.54.0"
  },
  "peerDependencies": {
    "wrangler": "4.54.0"
  }
}
