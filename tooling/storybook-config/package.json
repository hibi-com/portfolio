{
  "private": true,
  "version": "2.0.0",
  "name": "@portfolio/storybook-config",
  "type": "module",
  "types": "./dist/main.preset.d.ts",
  "exports": {
    "./main": {
      "types": "./dist/main.preset.d.ts",
      "import": "./dist/main.preset.js",
      "require": "./dist/main.preset.js"
    },
    "./preview": {
      "types": "./dist/preview.preset.d.ts",
      "import": "./dist/preview.preset.js",
      "require": "./dist/preview.preset.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.js",
      "require": "./dist/theme.js"
    }
  },
  "scripts": {
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun build ./src/main.preset.ts ./src/preview.preset.tsx ./src/theme.ts --outdir dist --target node --format esm --splitting --external react --external react-dom --external storybook --external @storybook/* --external vite --external lightningcss",
    "build:types": "git clean -xdf .cache/tsbuildinfo.json && tsc --emitDeclarationOnly --declaration --outDir dist",
    "clean:cache": "git clean -xdf .cache .turbo",
    "clean:pkg": "git clean -xdf node_modules dist",
    "dev": "bun run build:js --watch",
    "fmt": "biome format --write .",
    "fmt:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.15",
    "@storybook/theming": "8.6.14",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "7.11.0",
    "vite": "6.1.0",
    "vite-tsconfig-paths": "6.0.3"
  },
  "devDependencies": {
    "@portfolio/biome-config": "workspace:*",
    "@portfolio/tsconfig": "workspace:*",
    "storybook": "8.6.14",
    "typescript": "5.9.3"
  }
}
